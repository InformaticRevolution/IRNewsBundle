{
    "name": "informaticrevolution/news-bundle",
    "type": "symfony-bundle",
    "description": "Symfony IRNewsBundle",
    "keywords": ["news", "article"],
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Kirsch",
            "email": "informatic.revolution@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "stof/doctrine-extensions-bundle": "1.1.*"
    },
    "require-dev": {
        "symfony/finder": "*",
        "symfony/twig-bundle": "*",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "doctrine/doctrine-bundle": "*@dev",
        "doctrine/orm": "*",
        "nelmio/alice": "*"
    },     
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "IR\\Bundle\\NewsBundle": "" }
    },
    "target-dir": "IR/Bundle/NewsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
